{
  "name": "ssr-grpc-api",
  "version": "0.1.0",
  "description": "Nodejs GRPC API",
  "main": "dist/index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .ts .",
    "build": "rimraf dist && tsc",

    "@typescript-eslint/parser": "^4.28.4",
    "dtslint": "^4.1.2",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-sonarjs": "^0.9.1",
    "grpc_tools_node_protoc_ts": "^5.3.0",
    "grpc-tools": "^1.11.2",
    "protobufjs": "^6.11.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
